{
  "name": "intern-time-tracker",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:css": "tailwindcss -i ./src/index.css -o ./public/output.css",
    "watch:css": "tailwindcss -i ./src/index.css -o ./public/output.css --watch",
    "vite": "vite",
    "dev": "npm-run-all --parallel watch:css vite",
    "dev:backend": "cd backend && npm run dev",
    "dev:full": "npm-run-all --parallel watch:css vite dev:backend",
    "build": "npm run build:css && vite build",
    "serve": "vite preview",
    "preview": "vite preview",
    "netlify-dev": "netlify dev"
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.14",
    "netlify-cli": "^17.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.4.2",
    "vite": "^7.0.3"
  },
  "dependencies": {
    "@types/leaflet": "^1.9.20",
    "leaflet": "^1.9.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0"
  }
}
