{
  "name": "intern-time-&-absence-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run build:css\" \"vite\"",
    "build": "npm run build:css:prod && vite build",
    "build:css": "tailwindcss -i ./index.css -o ./output.css --watch",
    "build:css:prod": "tailwindcss -i ./index.css -o ./output.css",
    "vite": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  }
}
